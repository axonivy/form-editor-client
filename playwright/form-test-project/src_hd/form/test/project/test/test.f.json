{
  "$schema" : "https://json-schema.axonivy.com/form/13.1.0/form.json",
  "id" : "a5c1d16e-1d08-4e1f-a9f0-436c553a3881",
  "config" : {
    "theme" : "freya-ivy"
  },
  "components" : [ {
    "cid" : "Text1",
    "type" : "Text",
    "config" : {
      "content" : "This form uses a grid layout for address data and some Ivy Components."
    }
  }, {
    "cid" : "Input2",
    "type" : "Input",
    "config" : {
      "required" : "false",
      "label" : "Address",
      "value" : "#{data.address.street}"
    }
  }, {
    "cid" : "Layout3",
    "type" : "Layout",
    "config" : {
      "components" : [ {
        "cid" : "Input4",
        "type" : "Input",
        "config" : {
          "required" : "false",
          "label" : "City",
          "value" : "#{data.address.city}"
        }
      }, {
        "cid" : "Layout5",
        "type" : "Layout",
        "config" : {
          "components" : [ {
            "cid" : "Input6",
            "type" : "Input",
            "config" : {
              "required" : "false",
              "label" : "Zip",
              "value" : "#{data.address.zip}"
            }
          }, {
            "cid" : "Input7",
            "type" : "Input",
            "config" : {
              "required" : "false",
              "label" : "Country",
              "value" : "#{data.address.country}"
            }
          } ]
        }
      } ],
      "gridVariant" : "FREE"
    }
  }, {
    "cid" : "Composite8",
    "type" : "Composite",
    "config" : {
      "name" : "form.test.project.PersonComponent",
      "startMethod" : "start",
      "parameters" : {
        "person" : "#{data.person}"
      }
    }
  }, {
    "cid" : "Composite9",
    "type" : "Composite",
    "config" : {
      "name" : "form.test.project.AddressComponent",
      "startMethod" : "start",
      "parameters" : {
        "address" : "#{data.address}"
      }
    }
  }, {
    "cid" : "Layout10",
    "type" : "Layout",
    "config" : {
      "components" : [ {
        "cid" : "Button11",
        "type" : "Button",
        "config" : {
          "name" : "Cancel",
          "action" : "#{ivyWorkflowView.cancel()}",
          "variant" : "SECONDARY"
        }
      }, {
        "cid" : "Button12",
        "type" : "Button",
        "config" : {
          "name" : "Proceed",
          "action" : "#{logic.close}"
        }
      } ],
      "type" : "FLEX",
      "justifyContent" : "END"
    }
  } ]
}